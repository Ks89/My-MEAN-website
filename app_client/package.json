{
  "name": "stefanocappa-frontend",
  "description": "My M.E.A.N. website",
  "homepage": "https://www.mywebsite.fake.it",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "node_modules/webpack/bin/webpack.js --config webpack.prod.config.js --colors --progress --display-error-details --display-cached --profile",
    "buildDev": "node_modules/webpack/bin/webpack.js --config webpack.dev.config.js --colors --progress --display-error-details --display-cached --profile",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "serve:dist": "static dist -H '{\"Cache-Control\": \"no-cache, must-revalidate\"}' -z",
    "test": "karma start karma.conf.js",
    "predeploy": "npm run build && rimraf ../server/build/public && mkdirp ../server/build/public",
    "deploy": "copyup dist/* ../server/build/public"
  },
  "engines": {
    "npm": ">=3.10.3"
  },
  "dependencies": {
    "@angular/common": "^2.0.1",
